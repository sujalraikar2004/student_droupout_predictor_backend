{
    "version": 2,
    "builds": [
      { "src": "src/index.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "src/index.js" }
    ],
    "installCommand": "npm install",
    "functions": {
      "src/index.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }